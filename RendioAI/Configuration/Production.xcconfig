// Optionally include local secrets (not in git)
#include? "Production.xcconfig.local"

//
//  Production.xcconfig
//  RendioAI
//
//  Production environment configuration
//  ⚠️ WARNING: Do not commit production keys to git!
//  Use CI/CD secrets or secure storage for production keys
//

// Supabase Configuration
// ⚠️ IMPORTANT: Never commit production keys to git!
// Inject via CI/CD or a local override file: Production.xcconfig.local (ignored)
SUPABASE_URL = $(SUPABASE_URL)
SUPABASE_ANON_KEY = $(SUPABASE_ANON_KEY)

// API Configuration
API_TIMEOUT = 15
API_MAX_RETRIES = 2

// Environment
ENVIRONMENT_NAME = production
ENABLE_LOGGING = NO
ENABLE_DEBUG_MODE = NO

// Info.plist keys (will be injected into generated Info.plist)
INFOPLIST_KEY_SUPABASE_URL = $(SUPABASE_URL)
INFOPLIST_KEY_SUPABASE_ANON_KEY = $(SUPABASE_ANON_KEY)

