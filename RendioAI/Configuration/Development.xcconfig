// Optionally include local secrets (not in git)
#include? "Development.xcconfig.local"

//
//  Development.xcconfig
//  RendioAI
//
//  Development environment configuration
//  This file contains development-specific build settings
//

// Supabase Configuration
// Do NOT put real keys here. Use a local override file: Development.xcconfig.local (ignored)
SUPABASE_URL = $(SUPABASE_URL)
SUPABASE_ANON_KEY = $(SUPABASE_ANON_KEY)

// API Configuration
API_TIMEOUT = 30
API_MAX_RETRIES = 3

// Environment
ENVIRONMENT_NAME = development
ENABLE_LOGGING = YES
ENABLE_DEBUG_MODE = YES

// Info.plist keys (will be injected into generated Info.plist)
INFOPLIST_KEY_SUPABASE_URL = $(SUPABASE_URL)
INFOPLIST_KEY_SUPABASE_ANON_KEY = $(SUPABASE_ANON_KEY)

